    <script language="javascript" type="text/javascript">

        function AlfaNumeric(t) {
            var cod = " ";
            var v = cod
            var w = "";
            for (var i = 0; i < t.value.length; i++) {
                x = t.value.charAt(i);
                if (v.indexOf(x, 0) == -1)
                    w += x;
            }
            t.value = w;
        }


        function OnlyAlfa1(t) {
            var cod = "ABCDEFGHIJKLMNOPQRSTUVWXYZ abcdefghijklmnopqrstuvwxyz";
            var v = cod
            var w = "";
            for (var i = 0; i < t.value.length; i++) {
                x = t.value.charAt(i);
                if (v.indexOf(x, 0) != -1)
                    w += x;
            }
            t.value = w;
        }

        function AlfaNum1(t) {
            var cod = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
            var v = cod
            var w = "";
            for (var i = 0; i < t.value.length; i++) {
                x = t.value.charAt(i);
                if (v.indexOf(x, 0) != -1)
                    w += x;
            }
            t.value = w;
        }
        function checkonlySpace(t) {
            var val = t.value.replace(/^\s+|\s+$/, '');
            // alert(t.value.length);
            if (val.length == 0) {
                t.value = '';
                //t.focus();
            }
        }
        function isValidDate(txtDateObj) {

            var str = txtDateObj.value;
            if (str != '') {

                var arr = [];
                arr['Jan'] = 0;
                arr['Feb'] = 1;
                arr['Mar'] = 2;
                arr['Apr'] = 3;
                arr['May'] = 4;
                arr['Jun'] = 5;
                arr['Jul'] = 6;
                arr['Aug'] = 7;
                arr['Sep'] = 8;
                arr['Oct'] = 9;
                arr['Nov'] = 10;
                arr['Dec'] = 11;
                var arrDate = str.split('-');
                if (arrDate.length == 3 && str.length == 11) {
                    var d = new Date(arrDate[2], arr[arrDate[1]], arrDate[0]);
                    if (!isNaN(d) && d.getFullYear() == arrDate[2] && d.getMonth() == arr[arrDate[1]] && d.getDate() == arrDate[0]) {
                        //alert("its valid");
                        return true;
                    }
                }
                alert(str + "  " + "Not a valid Date Formate \n\n Enter as 01-Jan-2011");
                txtDateObj.value = '';
                return false;
            }
        }
        function ValidateVisaCountry() {
            var Nationality = document.getElementById('<%=ddlNationality.ClientID%>').value;
            var VisaAppliedFor = document.getElementById('<%=ddlCountry.ClientID%>').value;

            if (Nationality == VisaAppliedFor) {
                alert("Visa can not be apply for your own Country");
                return false;
            }
            else
                return true;
        }

        function NumNSign(t) {

            var cod = "+-0123456789";
            var v = cod
            var w = "";
            for (var i = 0; i < t.value.length; i++) {
                x = t.value.charAt(i);
                if (v.indexOf(x, 0) != -1)
                    w += x;
            }
            t.value = w;
        }

        function datevalidate() {
            var Fdate = document.getElementById('<%=txtArrivalDate.ClientID%>').value;
            var Edate = document.getElementById('<%=txtDepartureDate.ClientID%>').value;
            //         if (Fdate == "") {
            //             alert("Arrival Date requierd.")
            //             return false;
            //          }

            //         if (Edate == "") {
            //             alert("Departure Date requierd.")
            //             return false;
            //         }

            if (Date.parse(Fdate) > Date.parse(Edate)) {
                alert("Arrival Date should be greater than or equal to Departure Date.")
                return false;
            }
            else {
                return true;
            }
        }

    </script>
